    ] .

<https://data.digichecks.eu/def/isVerifiedIn> a owl:ObjectProperty;
  skos:prefLabel "is verified in";
  skos:definition "Is an association that indicates that the representer object represents the represented object."@en .

<https://w3id.org/sml/def#Requirement> a owl:Class;
  rdfs:subClassOf <https://w3id.org/sml/def#InformationObject>;
  skos:prefLabel "Requirement";
  skos:definition "A rule that should hold for something, defined via its attributes or relations"@en .

<https://w3id.org/sml/def#hasRequirement> a owl:ObjectProperty;
  skos:prefLabel "has requirement" .

<https://data.digichecks.eu/def/logicalOperator> a owl:DatatypeProperty;
  rdfs:range xsd:enumeration;
  skos:prefLabel "logical operator";
  skos:definition "Provides the possibility to indicate if certain conditions are complementary (AND) or supplementary (OR)."@en .

<https://data.digichecks.eu/def/role> a owl:DatatypeProperty;
  skos:prefLabel "role";
  skos:definition "Indicates in what capacity someone/something is linked to a project, permit, ..."@en .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/95414b38-5a62-3cf9-beed-42cbc73f4b9e>
  a rdf:List;
  rdf:first "OR";
  skos:prefLabel "Logical operator list for requirements";
  rdf:rest ("AND") .

<https://data.digichecks.eu/def/ProjectRequirement> a owl:Class;
  rdfs:subClassOf <https://w3id.org/sml/def#Requirement>;
  skos:prefLabel "Project Requirement";
  skos:definition "A requirement that is specific to projects / use case. It cannot be generalized over multiple projects / use cases."@en .

<https://data.digichecks.eu/def/hasParcel> a owl:ObjectProperty;
  skos:prefLabel "has parcel";
  skos:definition "Indicates the link between location and parcel."@en .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/6615257dfd34eb2f8e03f59a>
  a sh:NodeShape;
  sh:targetClass <http://www.w3.org/2006/vcard/ns#Address>;
  sh:property [
      sh:path <https://data.digichecks.eu/def/isAddressFor>;
      sh:maxCount 1;
      sh:minCount 1;
      sh:class <http://www.w3.org/ns/prov#Location>
    ], [
      sh:path skos:prefLabel;
      sh:datatype xsd:string
    ], [
      sh:path rdfs:comment;
      sh:datatype xsd:string
    ] .

<https://data.digichecks.eu/def/isAddressFor> a owl:ObjectProperty;
  owl:inverseOf <http://www.w3.org/2006/vcard/ns#hasAddress>;
  skos:prefLabel "is address for";
  skos:definition "Relation to check if the address is linked to a location."@en .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/64b000ca27b0605cddb2d646>
  a sh:NodeShape;
  sh:targetClass <https://w3id.org/sml/def#TopConcept>;
  sh:property [
      sh:path <https://w3id.org/sml/def#isDescribedBy>;
      sh:class <https://w3id.org/idsa/core/Resource>
    ], [
      sh:path <https://data.digichecks.eu/def/wasDescribedBy>;
      sh:class <https://w3id.org/idsa/core/Resource>
    ], [
      sh:path skos:prefLabel;
      sh:datatype xsd:string
    ], [
      sh:path rdfs:comment;
      sh:datatype xsd:string
    ] .

<https://data.digichecks.eu/def/wasDescribedBy> a owl:ObjectProperty;
  skos:prefLabel "was described by";
  skos:definition "Relation that is used to indicate that a concept was linked to certain resource in the past. this is meant to keep track of different versions of resources, keep history on all links ever made from concept to resource."@en .

<http://www.w3.org/2006/vcard/ns#hasAddress> a owl:ObjectProperty;
  skos:prefLabel "has address" .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/64d4a25fec98ce2e4c41a07d>
  a sh:NodeShape;
  sh:targetClass <https://data.digichecks.eu/def/Verification>;
  sh:property [
      sh:path <https://data.digichecks.eu/def/hasVerificationMethod>;
      sh:maxCount 1;
      sh:minCount 1;
      sh:class <https://data.digichecks.eu/def/VerificationMethod>
    ], [
      sh:path <http://www.w3.org/ns/prov#wasAssociatedWith>;
      sh:maxCount 1;
      sh:minCount 1;
      sh:class <http://www.w3.org/ns/prov#Agent>
    ], [
      sh:path <https://data.digichecks.eu/def/isInputFor>;
      sh:minCount 1;
      sh:class <https://data.digichecks.eu/def/Decision>
    ], [
      sh:path <https://data.digichecks.eu/def/hasAsOutput>;
      sh:or ([
            sh:class <https://data.digichecks.eu/def/Document>
          ] [
            sh:class sh:ValidationReport
          ])
    ] .

<https://data.digichecks.eu/def/isInputFor> a owl:ObjectProperty;
  skos:prefLabel "is input for";
  skos:definition "Indicates that a / multiple verifications where carried out as a foundation for certain decision taken."@en .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/34e82a97-fdd5-3176-ab61-f75c778939ca>
  a rdf:List;
  rdf:first "approved";
  skos:prefLabel "outcomes for Decision for permits";
  rdf:rest ("rejected" "approved with reservations") .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/64d5f40dec98ce2e4c41a100>
  a sh:NodeShape;
  sh:targetClass <http://www.w3.org/ns/prov#Agent>;
