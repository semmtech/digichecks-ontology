      sh:path <http://www.w3.org/ns/prov#actedOnBehalfOf>;
      sh:class <http://www.w3.org/ns/prov#Agent>
    ], [
      sh:path rdfs:comment;
      sh:datatype xsd:string
    ], [
      sh:path skos:prefLabel;
      sh:datatype xsd:string
    ] .

<https://data.digichecks.eu/def/GenericRequirement> a owl:Class;
  rdfs:subClassOf <https://w3id.org/sml/def#Requirement>;
  skos:prefLabel "Generic Requirement";
  skos:definition "A requirement that is not specific for any projects / use case."@en .

<http://hub.laces.tech/digichecks/private/production/top-level-permit-ontology/64da327fec98ce2e4c41a229>
